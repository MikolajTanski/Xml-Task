@model User;
@{

}
<div class="jumbotron">
    <h1> Users read from .XML file </h1>
    <table class="table">
        <thead>
            <th> Id </th>
            <th> Name </th>
            <th> Email </th>
            <th> Age </th>
        </thead>
        <tbody>
            <tr>
                <td> @Model.Id </td>
                <td> @Model.Name </td>
                <td> @Model.Email </td>
                <td> @Model.Age </td>
            </tr>
        </tbody>
    </table>
</div>